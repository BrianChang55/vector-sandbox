# =============================================================================
# Vector - Docker Services Configuration
# =============================================================================
# Copy this file to .env and customize ports to avoid conflicts
# =============================================================================

# Redis port (default: 6379)
# This is used by docker-compose and mise tasks (celery, flower)
REDIS_PORT=6379

# PostgreSQL port (default: 5432)
POSTGRES_PORT=5432

# PostgreSQL credentials
POSTGRES_DB=vector
POSTGRES_USER=vector
POSTGRES_PASSWORD=vector
