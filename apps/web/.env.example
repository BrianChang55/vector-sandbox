# =============================================================================
# Internal Apps Frontend - Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your values
# Vite requires all environment variables to be prefixed with VITE_
# =============================================================================

# -----------------------------------------------------------------------------
# API Configuration
# -----------------------------------------------------------------------------
# Backend API base URL (include /api/v1 suffix)
VITE_API_BASE_URL=http://localhost:8001/api/v1

# -----------------------------------------------------------------------------
# Monitoring & Analytics (Optional)
# -----------------------------------------------------------------------------
# Sentry - Error tracking (https://sentry.io)
# Create a new project and get the DSN
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=development

# Mixpanel - Product analytics (https://mixpanel.com)
VITE_MIXPANEL_TOKEN=
